{
    "name": "flobbos/laravel-image-cache",
    "description": "Replacement for the now abandoned imagecache package by Intervention",
    "type": "laravel",
    "require": {
        "php": "^8.1",
        "intervention/image": "^3.0",
        "illuminate/cache": "^10.0|^11.0|^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flobbos\\LaravelImageCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flobbos\\LaravelImageCache\\ImageCacheServiceProvider"
            ],
            "aliases": {
                "ImageCache": "Flobbos\\LaravelImageCache\\Facades\\ImageCache"
            }
        }
    },
    "files": [
        "config/imagecache.php"
    ],
    "authors": [
        {
            "name": "Alexej Krzewitzki",
            "email": "alexej@helloo.it"
        }
    ],
    "minimum-stability": "dev"
}